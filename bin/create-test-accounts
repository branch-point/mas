'use strict';

var co = require('co'),
    User = require('../server/models/user.js');

var names = [
    'Alfa User',
    'Beta User',
    'Gamma User',
    'Delta User',
    'Epsilon User',
    'Zeta User'
    ];

var emails = [
    'user1@mas.com',
    'user2@mas.com',
    'user3@mas.com',
    'user4@mas.com',
    'user5@mas.com',
    'user6@mas.com'
    ];

var nicks = [
    'user1',
    'user2',
    'user3',
    'user4',
    'user5',
    'user6'
    ];

co(function *() {
    for (var i = 0; i < names.length; i++) {
        console.log('Adding user: ' + names[i]);

        let user = new User({
            name: names[i],
            email: emails[i],
            nick: nicks[i],
            inuse: '1'
        }, {}, {});

        user.setFinalPasswordSha('12345');
        yield user.generateUserId();
        yield user.save();
    }

    process.exit(0);
})();
